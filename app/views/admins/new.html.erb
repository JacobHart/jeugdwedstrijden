<h1>New Admin</h1>


<%= form_for @admin do |f| %>


  <div>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

    <div>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>

<div>
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>
