<div class="page-header">
  <h1><%= @boat_type.name %> <small>who do you challenge?</small></h1>
</div>

<%= form_tag(boat_type_url(@boat_type), method: 'put') do %>

  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name, @boat_type.name %>
  </div>


  <div>
    <%= submit_tag "Update Boat Type" %>
  </div>

<% end %>
