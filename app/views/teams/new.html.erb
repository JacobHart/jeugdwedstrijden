<h1>New Team</h1>


<%= form_tag(teams_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :boat_type_id %><br />
    <%= text_field_tag :boat_type_id %>
  </div>

    <div>
    <%= label_tag :club_id %><br />
    <%= text_field_tag :club_id %>
  </div>

  
  <div>
    <%= submit_tag "Create Team" %>
  </div>
<% end %>
