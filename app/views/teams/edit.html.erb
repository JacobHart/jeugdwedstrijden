<h1>Editing Team #<%= @team.id %></h1>


<%= form_tag(team_url(@team), method: 'put') do %>

  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name, @team.name %>
  </div>

  <div>
    <%= label_tag :boat_type_id %><br />
    <%= text_field_tag :boat_type_id, @team.boat_type_id %>
  </div>

  <div>
    <%= label_tag :club_id %><br />
    <%= text_field_tag :club_id, @team.club_id %>
  </div>

  
  <div>
    <%= submit_tag "Update Team" %>
  </div>

<% end %>
